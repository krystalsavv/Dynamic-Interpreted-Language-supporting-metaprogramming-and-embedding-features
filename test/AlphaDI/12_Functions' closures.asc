/* functions' closures */

function f(x) {
	return (function () { 
				print(x, "\n");
				});
}

function ff(x) {
	return (function () { 			
				return (function () {return x+2;});
				});
}

inner_f1 = f(1);
inner_f2 = f(2);

inner_ff1 = ff(1);
inner_ff2 = ff(2);

inner_f1();					// 1.000
inner_f2();					// 2.000

print(inner_ff1()(), "\n"); // 3.000
print(inner_ff2()(), "\n"); // 4.000

inner_f1.x = 100;

inner_f1();					// 100.000
inner_f2();					// 2.000	